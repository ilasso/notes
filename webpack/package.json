{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entrypoint/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./pluggins/webpack.config.js",
    "build:dev": "webpack-dev-server  --config ./webpack-dev-server/webpack.config.js",
    "start-dev": "webpack serve --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel/webpack.config.js",
    "start-babel": "webpack serve --config ./babel/webpack.config.js",
    "start-react": "webpack serve --config ./react/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@ilasso",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.12.13",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/plugin-transform-runtime": "7.12.15",
    "@babel/preset-env": "7.12.16",
    "@babel/preset-react": "7.12.13",
    "babel-loader": "8.2.2",
    "css-loader": "5.0.2",
    "html-webpack-plugin": "5.0.0",
    "mini-css-extract-plugin": "1.3.6",
    "source-map-loader": "2.0.1",
    "style-loader": "2.0.0",
    "webpack": "5.21.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  }
